{
  "name": "bplustree-ts",
  "version": "0.0.1",
  "description": "bplustree implementation in typescript",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "ci:format": "yarn run format && yarn run diff-check",
    "ci:test": "yarn run jest --ci --silent --runInBand",
    "coverage": "yarn run jest --collectCoverage",
    "diff-check": "git diff --exit-code --name-only ./src",
    "format": "prettier --config .prettierrc --write 'src/**/*.ts'",
    "jest": "jest -c jest.config.js",
    "test": "yarn run jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
